<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <!-- Finnish salary rules. Ordered by code -->

        <!-- Tuntipalkka -->
        <record id="hr_payroll.hr_rule_basic" model="hr.salary.rule">
            <field name="name">Tuntipalkka</field>
            <field name="category_id" ref="hr_payroll.BASIC" />
            <field name="code">1</field>
            <field name="sequence">1</field>
            <field name="condition_select">none</field>
            <field name="quantity">1.0</field>
            <field name="appears_on_payslip" eval="True" />
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = (inputs.A11 and inputs.A11.amount)+(inputs.A12 and inputs.A12.amount)+(inputs.A13 and inputs.A13.amount)+(inputs.A14 and inputs.A14.amount)+(inputs.A15 and inputs.A15.amount)+(inputs.A16 and inputs.A16.amount)+(inputs.A17 and inputs.A17.amount)+(inputs.A18 and inputs.A18.amount)+(inputs.A19 and inputs.A19.amount)+(inputs.A191 and inputs.A191.amount)</field>
        </record>

        <!-- Ylityölisä 50% -->
        <record id="hr_salary_rule_11" model="hr.salary.rule">
            <field name="name">Ylityölisä 50%</field>
            <field name="category_id" ref="hr_salary_rule_category_vahennykset" />
            <field name="code">11</field>
            <field name="sequence">5</field>
            <field name="condition_select">none</field>
            <field name="quantity">1.0</field>
            <field name="appears_on_payslip" eval="True" />
            <field name="amount_select">code</field>
            <field name="amount_python_compute"></field>
        </record>

        <!-- KM-korvaus lisähenkilö -->
        <record id="hr_salary_rule_101" model="hr.salary.rule">
            <field name="name">KM-korvaus lisähenkilö</field>
            <field name="category_id" ref="hr_payroll.ALW" />
            <field name="code">101</field>
            <field name="sequence">5</field>
            <field name="condition_select">none</field>
            <field name="quantity">1.0</field>
            <field name="appears_on_payslip" eval="True" />
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = (inputs.A1011 and inputs.A1011.amount)+(inputs.A1012 and inputs.A1012.amount)+(inputs.A1013 and inputs.A1013.amount)+(inputs.A1014 and inputs.A1014.amount)+(inputs.A1015 and inputs.A1015.amount)</field>
            <field name="register_id" ref="hr_contribution_register_korvaukset" />
        </record>

        <!-- Säähaittakorvaus 3% -->
        <record id="hr_salary_rule_102" model="hr.salary.rule">
            <field name="name">Säähaittakorvaus 3%</field>
            <field name="category_id" ref="hr_payroll.BASIC" />
            <field name="code">102</field>
            <field name="sequence">5</field>
            <field name="condition_select">none</field>
            <field name="quantity">1.0</field>
            <field name="appears_on_payslip" eval="True" />
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = (inputs.A1021 and inputs.A1021.amount)+(inputs.A1022 and inputs.A1022.amount)+(inputs.A1023 and inputs.A1023.amount)+(inputs.A1024 and inputs.A1024.amount)+(inputs.A1025 and inputs.A1025.amount)</field>
        </record>

        <!-- Ansiontasaus -->
        <record id="hr_salary_rule_103" model="hr.salary.rule">
            <field name="name">Ansiontasaus</field>
            <field name="category_id" ref="hr_payroll.BASIC" />
            <field name="code">103</field>
            <field name="sequence">5</field>
            <field name="condition_select">none</field>
            <field name="quantity">1.0</field>
            <field name="appears_on_payslip" eval="True" />
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = result = (inputs.A1031 and inputs.A1031.amount)+(inputs.A1032 and inputs.A1032.amount)+(inputs.A1033 and inputs.A1033.amount)+(inputs.A1034 and inputs.A1034.amount)+(inputs.A1035 and inputs.A1035.amount)</field>
        </record>

        <!-- Sairasajanpalkka -->
        <record id="hr_salary_rule_104" model="hr.salary.rule">
            <field name="name">Sairasajanpalkka</field>
            <field name="category_id" ref="hr_payroll.BASIC" />
            <field name="code">103</field>
            <field name="sequence">5</field>
            <field name="condition_select">none</field>
            <field name="quantity">1.0</field>
            <field name="appears_on_payslip" eval="True" />
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = (inputs.A1041 and inputs.A1041.amount)+(inputs.A1042 and inputs.A1042.amount)+(inputs.A1043 and inputs.A1043.amount)+(inputs.A1044 and inputs.A1044.amount)+(inputs.A1045 and inputs.A1045.amount)</field>
            <field name="register_id" ref="hr_contribution_register_sairasajanpalkka" />
        </record>

        <!-- Vuokra -->
        <record id="hr_salary_rule_107" model="hr.salary.rule">
            <field name="name">Vuokra</field>
            <field name="category_id" ref="hr_salary_rule_category_vahennykset" />
            <field name="code">107</field>
            <field name="sequence">5</field>
            <field name="condition_select">none</field>
            <field name="quantity">1.0</field>
            <field name="appears_on_payslip" eval="True" />
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = (inputs.Vuokra1 and inputs.Vuokra1.amount)+(inputs.Vuokra2 and inputs.Vuokra2.amount)+(inputs.Vuokra3 and inputs.Vuokra3.amount)+(inputs.Vuokra4 and inputs.Vuokra4.amount)</field>
        </record>

        <!-- Rahapalkka -->
        <record id="hr_payroll.hr_rule_taxable" model="hr.salary.rule">
            <field name="name">Rahapalkka</field>
            <field name="category_id" ref="hr_payroll.GROSS" />
            <field name="code">Brutto</field>
            <field name="sequence">20</field>
            <field name="condition_select">none</field>
            <field name="quantity">1.0</field>
            <field name="appears_on_payslip" eval="False" />
            <field name="amount_select">code</field>
            <field name="amount_python_compute">result = categories.BASIC</field>
        </record>

        <!-- Nettopalkka -->
        <record id="hr_payroll.hr_rule_net" model="hr.salary.rule">
            <field name="name">Nettopalkka</field>
            <field name="category_id" ref="hr_payroll.NET" />
            <field name="code">NET</field>
            <field name="sequence">1000</field>
            <field name="condition_select">none</field>
            <field name="quantity">1.0</field>
            <field name="appears_on_payslip" eval="False" />
            <field name="amount_select">code</field>
            <field name="amount_python_compute"> result = categories.BASIC + categories.ALW - categories.Vero - categories.LAK - ((contract.membership_fee/100)*categories.Palkka) - categories.VAH </field>
        </record>

    </data>
</openerp>